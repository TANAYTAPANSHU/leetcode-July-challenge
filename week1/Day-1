Arranging Coins

var arrangeCoins = function(n) {
    let i=1;    
    while(n>=0)
        {
            if(n>=i)
                {
                              n=n-i;     

                    i++;
                }
            else
                {
                    
                   return i-1;
                  
                }
            
        }
    
    
    
    
};
